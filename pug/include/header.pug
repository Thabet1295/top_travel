                                    div(class="form-group text-center")
                                        h4 Login Form
                                    div(class="form-group ", style="position: relative;")
                                        label(for="l_email" class="d-flex") 
                                            input(type="email", id="l_email", class="form-control", placeholder="username", required)
                                            i(class="fa fa-envelope", aria-hidden="true")
                                    div(class="form-group pb-3", style="position: relative;")
                                        label(for="l_password" class="d-flex") 
                                            input(type="password", id="l_password", class="form-control", placeholder="Pawswword ?", required)
                                            i(class="fa fa-unlock-alt", aria-hidden="true")
                                        a(href="#forgotPassword", style="display:block; position: absolute; right: 0;", title="Fill Email Field and Click it")
                                            | Forgot Password?
                                    div(class="form-group pt-2")
                                        button(class="btn btn-info form-control") Login
                                    div(class="text-center pt-2 social-login")
                                        h6 OR Continue with
                                        a(href="#", class="google")
                                            i(class="fa fa-google-plus fa-lg")
                                        a(href="#", class="facebook")
                                            i(class="fa fa-facebook fa-lg")
                                        a(href="#", class="twitter")
                                            i(class="fa fa-twitter fa-lg")
                                        a(href="#", class="github")
                                            i(class="fa fa-github fa-lg")
                                        // Registeration Modal
                #register(class="modal fade")
                    div(class="modal-dialog modal-dialog-centered", role="document")
                        div(class="modal-content form-wrapper")
                            div(class="close-box", data-dismiss="modal")
                                i(class="fa fa-times fa-2x")
                            div(class="container-fluid mt-5")
                                form#RegisterationForm(action="#", method="post")
                                    div(class="form-group text-center pb-2")
                                        h4 Registration Form
                                    
                                    div(class="form-group ", style="position: relative;")
                                        label(for="l_user" class="d-flex") 
                                            input(type="text", id="l_user", class="form-control", placeholder="username", required)
                                            i(class="fa fa-user", aria-hidden="true")
                                    div(class="form-group", style="position: relative;")
                                        label(for="l_email" class="d-flex") 
                                            input(type="email", id="l_email", class="form-control", placeholder="Email", required)
                                            i(class="fa fa-envelope", aria-hidden="true")
                                    div(class="form-group", style="position: relative;")
                                        label(for="l_password" class="d-flex") 
                                            input(type="password", id="l_password", class="form-control", placeholder="Pawswword ?", required)
                                            i(class="fa fa-unlock-alt", aria-hidden="true")
                                    div(class="form-group pb-3", style="position: relative;")
                                        label(for="l_cpassowrd" class="d-flex") 
                                            input(type="cpassword", id="l_cpassowrd", class="form-control", placeholder="Confirm Password", required)
                                            i(class="fa fa-unlock-alt", aria-hidden="true")        

                                    //- div(class="form-group col-6", style="position:relative;")
